  },
  "homepage": "https://github.com/cheminfo/stock-manager-app#readme",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "copy-webpack-plugin": "^4.0.0",
    "jquery": "^3.1.1",
    "minimist": "^1.2.0",
    "on-build-webpack": "^0.1.0",
